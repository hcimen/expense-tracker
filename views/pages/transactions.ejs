<%- include('../partials/header') -%>

<%- include('../partials/navigation') -%>

<div class="addTransaction" id="addTransactionMain"> All Transactions <br>


<% for (entry of allTransactions) { %>
	<div style="font-size: medium; border: solid; border-collapse: collapse; border-color: black; ">
		<form action="#">
			<table>
				<tr>
					<th> Date</th>
					<th> Description </th>
					<th> Amount</th>
					<th> Type</th>
					<th> Category </th>
					<th> Account Name </th>
					<th> Labels </th>
					<th> Notes </th>
				</tr>
				<tr>
					<td> <%= entry.date %> </td>
					<td> <%= entry.description %> </td>
					<td> <%= entry.amount %> </td>
					<td> <%= entry.transaction_type %> </td>
					<td> <%= entry.category %> </td>
					<td> <%= entry.account_name %> </td>
					<td> <%= entry.labels %> </td>
					<td> <%= entry.notes %> </td>
				</tr>
			</table>
		</form>
	</div>
<% } %>	

</div>

<%- include('../partials/footer') -%>